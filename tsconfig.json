{
  "compilerOptions": {
    "target": "ES2020",
	  "emitDeclarationOnly": false,
    "module": "ESNext",
    "strict": true,
    "outDir": "dist",
    "suppressImplicitAnyIndexErrors": true,
    "strictPropertyInitialization": false,
    "allowJs": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}
